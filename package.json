{
  "name": "livestar_insurance",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "dev:with-db": "npm-run-all -p db:up:dev prisma:push:dev dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "seed": "cross-env ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "cross-env ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/prisma-migrate.ts",
    "prisma:push:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/prisma-push.ts",
    "prisma:push:test": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/prisma-push.ts",
    "prisma:reset": "cross-env ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/prisma-reset.ts",
    "db:up:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/db-up.ts",
    "db:down:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/db-down.ts",
    "db:up:test": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/db-up.ts",
    "db:down:test": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.scripts.json scripts/db-down.ts",
    "test:e2e:dev": "cross-env NODE_ENV=test npm-run-all -s db:up:test prisma:push:test prisma:generate test:e2e:run db:down:test",
    "test:e2e:run": "cross-env NODE_ENV=test dotenv -e .env.test -- playwright test"
  },
  "dependencies": {
